주요 엔티티 필드명
typescript// 이 필드명을 BE/FE 모두 동일하게 사용
interface Board {
  boardId: number;
  boardName: string;
  teamId: number;
  createdAt: string;
  updatedAt: string;
}

interface Feature {
  featureId: number;
  boardId: number;
  featureName: string;
  status: "TODO" | "IN_PROGRESS" | "DONE";
  orderIndex: number;
}

interface Task {
  taskId: number;
  featureId: number;
  taskName: string;
  assigneeId: number | null;
  status: "TODO" | "IN_PROGRESS" | "DONE";
  orderIndex: number;
}

interface Member {
  memberId: number;
  teamId: number;
  userId: number;
  role: "OWNER" | "ADMIN" | "MEMBER";
  displayName: string;
}
```

